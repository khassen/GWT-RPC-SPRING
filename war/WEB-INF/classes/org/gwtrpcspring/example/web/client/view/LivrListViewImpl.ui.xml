<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.cellTable {
		width: 90%;
		height: 80%;
		border: 1px solid #ccc;
	}
	</ui:style>
	<g:HTMLPanel>
		<table width="100%">
			<tr>
				<td>
					<td valign='top'></td>
					<c:CellTable addStyleNames='{style.cellTable}'
						ui:field="cellTable" />
				</td>
			</tr>
			<tr>
				<td>
					<c:SimplePager ui:field="pager" />
				</td>
			</tr>
			<td align="center">
				<tr>
					<g:DecoratorPanel>
						<g:HorizontalPanel styleName="aucentre" spacing="10">
						   <g:Button width="107" text="Modifier" ui:field="modifyButton"/>
						   <g:Button text="Supprimer" ui:field='deleteButton' />
						</g:HorizontalPanel>
					</g:DecoratorPanel>
				</tr>
			</td>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 